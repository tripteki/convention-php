{
    "name": "tripteki/laravelphp-lint",
    "version": "1.0.1",
    "description": "Trip Teknologi's Codebase Convention",

    "readme": "README.md",
    "license": "MIT",
    "authors": [ { "name": "Trip Teknologi", "email": "tripteki.company@gmail.com" } ],
    "homepage": "https://github.com/tripteki/convention-php",
    "support": { "issues": "https://github.com/tripteki/convention-php/issues" },

    "require": {

        "php": "^8.0.2",

        "laravel/framework": "^9.19",
        "laravel/pint": "^1.0.2"
    },

    "require-dev": {},

    "autoload": {

        "psr-4": {

            "Tripteki\\Lint\\": "src/"
        }
    },

    "autoload-dev": {},

    "extra": {

        "laravel": {

            "dont-discover": [],

            "providers": [

                "Tripteki\\Lint\\Providers\\LintServiceProvider"
            ],

            "aliases": []
        }
    }
}
